@model WebBannuochoa.Models.HANG

@{
    ViewBag.Title = "themhang";
    Layout = "~/Views/Admin/LayoutAdmin.cshtml";
}

<div class="content-box-header">
    <div class="panel-title">THÊM HÃNG NƯỚC HOA:</div>
</div>

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
                Tên Hãng:
            <div class="col-md-10">
                @Html.EditorFor(model => model.TenH, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.TenH, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
                Nhập khẩu:
             <div class="col-md-10">
                @Html.EditorFor(model => model.NhapKhau, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.NhapKhau, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="themhang" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Trở về", "HangSX")
</div>
